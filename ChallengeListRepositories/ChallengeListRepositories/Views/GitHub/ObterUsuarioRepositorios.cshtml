@model IEnumerable<ConsumoGitWebService.DTO.RepositorioGitDTO>

@{
    ViewBag.Title = "ObterUsuarioRepositorios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="panel panel-info" style="margin:50px;">
    <div class="panel-heading">
        <h2>Repositorios do usuário - @ViewBag.UsuarioGit</h2>

    </div>
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Url)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CloneUrl)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Url)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CloneUrl)
                    </td>

                </tr>
            }

        </table>
    </div>
</div>



